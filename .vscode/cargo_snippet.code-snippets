      "            let mut base = base.clone();",
      "            let mut ans = Self::identity();",
      "            let mut n = n;",
      "            while n > 0 {",
      "                if n & 1 == 1 {",
      "                    ans = Self::binary_operation(&ans, &base);",
      "                }",
      "                base = Self::binary_operation(&base, &base);",
      "                n >>= 1;",
      "            }",
      "            ans",
      "        }",
      "    }",
      "    impl<T> MonoidExtPow for T where T: Monoid {}",
      "}"
