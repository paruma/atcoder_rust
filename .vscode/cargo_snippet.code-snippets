    "scope": "rust",
    "prefix": "make_adj_from_undirected",
    "body": [
      "pub fn make_adj_from_undirected(n_vertex: usize, edges: &[(usize, usize)]) -> Vec<Vec<usize>> {",
      "    let mut adj = vec![vec![]; n_vertex];",
      "    for &(from, to) in edges {",
      "        adj[from].push(to);",
      "        adj[to].push(from);",
      "    }",
      "    adj",
      "}"
    ]
