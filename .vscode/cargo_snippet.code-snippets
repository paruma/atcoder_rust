      "use monoid_transform::*;",
      "pub mod dynamic_monoid {",
      "    pub trait DynamicMonoid {",
      "        type S: Clone;",
      "        fn identity(&self) -> Self::S;",
      "        fn binary_operation(&self, a: &Self::S, b: &Self::S) -> Self::S;",
      "        /// base^n を求める",
