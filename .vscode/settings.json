{
    "rust-analyzer.linkedProjects": [
        "./Cargo.toml",
    ],
    "cSpell.words": [
        "chmax",
        "chmin",
        "cumsum",
        "Deque",
        "fastout",
        "fromisizee",
        "iproduct",
        "Itertools",
        "izip",
        "lazysegtree",
        "peekable",
        "powerset",
        "Procon",
        "proconio",
        "rngs",
        "Segtree",
        "struct",
        "yesno"
    ],
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.check.extraArgs": [
        "--",
        "-A",
        "clippy::needless_range_loop",
        "-A",
        "clippy::too_many_arguments",
        "-A",
        "clippy::collapsible_else_if",
    ],
}
