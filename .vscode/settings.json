{
    "rust-analyzer.linkedProjects": [
        "./Cargo.toml",
    ],
    "cSpell.words": [
        "Abelian",
        "bitand",
        "chmax",
        "chmin",
        "cnts",
        "cumsum",
        "Deque",
        "fastout",
        "Fenwick",
        "fromisizee",
        "iproduct",
        "Itertools",
        "izip",
        "lazysegtree",
        "multiset",
        "mutilset",
        "peekable",
        "potentialized",
        "powerset",
        "Procon",
        "proconio",
        "Reroot",
        "rngs",
        "Segtree",
        "struct",
        "sympy",
        "yesno"
    ],
    "rust-analyzer.cargo.buildScripts.overrideCommand": [
        "cargo",
        "clippy",
        "--quiet",
        "--workspace",
        "--message-format=json",
        "--all-targets",
        "--",
        "-A",
        "clippy::needless_range_loop",
        "-A",
        "clippy::too_many_arguments",
        "-A",
        "clippy::collapsible_if",
        "-A",
        "clippy::collapsible_else_if",
        "-A",
        "clippy::if_same_then_else",
        "-A",
        "clippy::int_plus_one"
    ],
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "clippy",
        "--workspace",
        "--message-format=json",
        "--all-targets",
        "--",
        "-A",
        "clippy::needless_range_loop",
        "-A",
        "clippy::too_many_arguments",
        "-A",
        "clippy::collapsible_if",
        "-A",
        "clippy::collapsible_else_if",
        "-A",
        "clippy::if_same_then_else",
        "-A",
        "clippy::int_plus_one"
    ]
}
